    "data_hour = df.groupBy('hour').agg(\n",
    "    F.count('clk').alias('展示量'),\n",
    "    F.sum('clk').alias('点击量'),\n",
    "    F.mean('clk').alias('点击率')\n",
    ")\n",
    "data_hour = data_hour.orderBy(\"hour\")\n",
    "data_hour.show(3)\n",
    "data_hour = data_hour.toPandas()\n",
    "data_hour.to_csv(\"data_hour.csv\",index=False)"
   ]
