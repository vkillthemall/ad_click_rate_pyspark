   "source": [
    "# 建立《2017/05/05至2017/05/13的广告点击情况》的柱形-折线图\n",
    "from pyecharts import options as opts\n",
    "from pyecharts.charts import Bar, Line\n",
    "from pyecharts.faker import Faker\n",
    "name = data_date.date.tolist()\n",
    "v1 = data_date.展示量.tolist()\n",
    "v2 = data_date.点击量.tolist()\n",
    "v3 = ((data_date.点击率.values)*100).tolist()\n",
    "v3 = [round(i,2) for i in v3]\n",
    "\n",
    "bar = (\n",
    "    Bar(init_opts=opts.InitOpts(width=\"800px\", height=\"420px\"))\n",
    "    .add_xaxis(name)\n",
    "    .add_yaxis(\"展示量\", v1)\n",
    "    .add_yaxis(\"点击量\", v2)\n",
    "    .extend_axis(\n",
    "        yaxis=opts.AxisOpts(axislabel_opts=opts.LabelOpts(formatter=\"{value} %\"), interval=1, max_=6)\n",
    "    )\n",
    "    .set_series_opts(label_opts=opts.LabelOpts(is_show=True))\n",
    "    .set_global_opts(\n",
    "        xaxis_opts=opts.AxisOpts(axislabel_opts=opts.LabelOpts(rotate=-15)),\n",
    "        title_opts=opts.TitleOpts(title=\"2017/05/05至2017/05/13的广告点击情况\",pos_left=\"18%\"),\n",
    "        yaxis_opts=opts.AxisOpts(axislabel_opts=opts.LabelOpts(formatter=\"{value}\"), max_=100000),\n",
    "        legend_opts=opts.LegendOpts(pos_top=\"10%\", pos_right=\"10%\")\n",
    "    )\n",
    "\n",
    ")\n",
    "\n",
    "line = Line().add_xaxis(name).add_yaxis(\"点击率\", v3, yaxis_index=1)\n",
    "bar.overlap(line)\n",
    "bar.render_notebook()"
   ]
  },
