   "source": [
    "data_date = df.groupBy('date').agg(\n",
    "    F.count('clk').alias('展示量'),\n",
    "    F.sum('clk').alias('点击量'),\n",
    "    F.mean('clk').alias('点击率')\n",
    ")\n",
    "data_date = data_date.orderBy(\"date\")\n",
    "data_date.show(3)\n",
    "data_date = data_date.toPandas()\n",
    "data_date.to_csv(\"data_date.csv\",index=False)"
   ]
