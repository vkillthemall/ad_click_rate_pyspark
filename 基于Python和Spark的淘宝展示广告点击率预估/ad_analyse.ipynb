    "def clk_counts(df):\n",
    "    counts = df.groupBy(\"clk\").count()\n",
    "    total = df.count()\n",
    "    data_clk = counts.withColumn(\"Ratio(%)\", F.round((F.col(\"count\") / total * 100),2))\n",
    "    return data_clk\n",
    "data_clk = clk_counts(df)\n",
    "data_clk.show(3)\n",
    "data_clk = data_clk.toPandas()\n",
    "data_clk.to_csv(\"data_clk.csv\",index=False)"
   ]
